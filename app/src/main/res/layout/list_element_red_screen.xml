<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="match_parent"
		android:layout_height="80dp"
		android:layout_margin="2dp"
		android:background="@drawable/list_element_border_green">
	<TextView
			android:layout_width="280dp"
			android:layout_height="wrap_content"
			android:textSize="20sp"
			android:text="title"
			android:layout_marginLeft="5dp"
			android:layout_marginTop="5dp"
			android:id="@+id/title"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="20sp"
			android:text="n"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginRight="80dp"
			android:layout_marginTop="5dp"
			android:visibility="gone"
			android:id="@+id/n"/>
	<ImageView
			android:layout_width="35dp"
			android:layout_height="35dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginRight="80dp"
			android:src="@drawable/arrow_dropdown"
			android:visibility="gone"
			android:id="@+id/dropdownImg"/>
	<ImageView
			android:layout_width="45dp"
			android:layout_height="45dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginTop="5dp"
			android:layout_marginRight="10dp"
			android:src="@drawable/red_close_button"
			android:id="@+id/deleteBtn"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_alignParentBottom="true"
			android:layout_marginRight="15dp"
			android:layout_marginBottom="5dp"
			android:id="@+id/price"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="15sp"
			android:text="tag"
			android:layout_alignParentBottom="true"
			android:layout_centerHorizontal="true"
			android:layout_marginBottom="10dp"
			android:visibility="gone"
			android:id="@+id/tag1"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="15sp"
			android:text="tag2"
			android:layout_alignParentBottom="true"
			android:layout_toLeftOf="@+id/tag1"
			android:layout_marginBottom="10dp"
			android:layout_marginRight="5dp"
			android:visibility="gone"
			android:id="@+id/tag2"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="15sp"
			android:text="tag3"
			android:layout_alignParentBottom="true"
			android:layout_toRightOf="@+id/tag1"
			android:layout_marginBottom="10dp"
			android:layout_marginLeft="5dp"
			android:visibility="gone"
			android:id="@+id/tag3"/>
</RelativeLayout >